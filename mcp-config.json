{
  "mcpServers": {
    "weather": {
      "command": "python",
      "args": [
        "-m",
        "mcp_weather_server"
      ],
      "env": {
        "MCP_LOG_LEVEL": "ERROR"
      }
    },
    "wikipedia": {
      "command": "npx",
      "args": [
        "-y",
        "wikipedia-mcp-server"
      ],
      "env": {
        "MCP_LOG_LEVEL": "ERROR"
      }
    }
  }
}
